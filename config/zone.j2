$TTL	86400
@	IN	SOA  ns1.delivr.dev. root.delivr.dev. (
         {{ serial }}     ; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			  86400 )	; Negative Cache TTL
@	IN	NS	ns1.delivr.dev.
@	IN	NS	ns2.delivr.dev.

{% for record in records -%}
{{ record["domain"] }}    {{ record["ttl"] }}    IN    {{ record["type"] }}    {{ record["value"] }}
{% endfor %}